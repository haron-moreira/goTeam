<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.mobile.goteam"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="30"
        android:targetSdkVersion="32" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/globo96"
        android:label="@string/app_name"
        android:roundIcon="@drawable/globo96"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.GoTeam"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="br.com.mobile.goteam.DetalhesActivity"
            android:exported="false" />
        <activity
            android:name="br.com.mobile.goteam.SobreActivity"
            android:exported="false" />
        <activity
            android:name="br.com.mobile.goteam.CadastroActivity"
            android:exported="false" />
        <activity
            android:name="br.com.mobile.goteam.HomeScreenActivity"
            android:exported="false"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="br.com.mobile.goteam.LogActivity"
            android:exported="false" />
        <activity
            android:name="br.com.mobile.goteam.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="br.com.mobile.goteam.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>